if keyboard_check(vk_up) {
    y -= 5; // Move o objeto para cima (ajuste o valor 5 para alterar a velocidade)
}

// Verifica se a tecla para baixo está pressionada
if keyboard_check(vk_down) {
    y += 5; // Move o objeto para baixo (ajuste o valor 5 para alterar a velocidade)
}